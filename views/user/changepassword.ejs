<div class="container  w-75 mt-5 mb-5 pt-5 pb-5">
    <form action="/updatePassword" method="post">
    <div class="row">
      <div class="col">
        <h1 class="d-flex justify-content-center mb-3 text-primary p-2 ">EDIT PASSWORD </h1>
        <br>
       
      
        <div class="input-group mb-3 w-50">
          <span
            class="input-group-text bg-primary text-dark"
            id="inputGroup-sizing-default"
            >New Password</span
          >
          <input
            type="password"
            class="form-control"
            aria-label="Sizing example input"
            aria-describedby="inputGroup-sizing-default"
            name="password"
            id="newPassword"
          />
          
        
        </div>
        
        <div class="input-group mb-3 w-50">
          <span
            class="input-group-text bg-primary text-dark"
            id="inputGroup-sizing-default"
            >Confirm Password</span
          >
          <input
            type="text"
            class="form-control"
            aria-label="Sizing example input"
            aria-describedby="inputGroup-sizing-default"
          />
        </div>
        
    
      </div>
    </div>
    <button type="submit" class="btn btn-primary">SAVE CHANGES</button>
  </form>
  </div>
  
  
  